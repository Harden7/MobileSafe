<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="23dp"
        android:background="#ff286b8b" />

    <TextView
        android:id="@+id/title_back"
        style="@style/TitleStyle"
        android:text=" 《   设置中心" />

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#aacfcfcf" />

    <com.yan.mobilesafe.View.SettingItemView
        android:id="@+id/setting_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.yan.mobilesafe.View.SettingItemView
        android:id="@+id/setting_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.yan.mobilesafe.View.SettingClickItemView
        android:id="@+id/setting_address_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.yan.mobilesafe.View.SettingClickItemView
        android:id="@+id/setting_address_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.yan.mobilesafe.View.SettingItemView
        android:id="@+id/setting_black_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>
